// @title Angel Vision beat challenge # 1 @by xetekk
$: s("bd").beat("0,4,8,12",16)
$: s("cp").beat("4,12",16)
  .lpf(800)
  .room(0.8)
  .delay(0.8)

$: note("<f c f c c#>*16").s("supersaw")
  .gain(.7)
  .decay(0.4)
  .room(.5)
  .lpenv(1)
  .vib(2)
  .cutoff(sine.range(500, 4000).slow(6))

$: note("<[1 3 5 7?]>*2").s("supersaw").scale("c:major").trans(-24)
  .lpf(500)
  .gain(.9)
  .pdecay(".4")
  .distort("0 0.2 0.3")
  .room(.4)
  .cutoff(tri.range(500, 1600).slow(2))
